import java.util.*;
class Palindrome {
    public static void main(String[] a) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        int l=0, r=s.length()-1;
        while(l<r) {
            if(s.charAt(l)!=s.charAt(r)) {
                System.out.println("Not palindrome");
                return;
            }
            l++; r--;
        }
        System.out.println("Palindrome");
    }
}
