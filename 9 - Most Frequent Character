import java.util.*;
class MostFrequent {
    public static void main(String[] a) {
        Scanner sc = new Scanner(System.in);
        String s = sc.nextLine();
        int[] cnt = new int[256];
        for(char ch : s.toCharArray()) cnt[ch]++;
        int max = 0; char res = ' ';
        for(int i=0;i<256;i++)
            if(cnt[i]>max) { max=cnt[i]; res=(char)i; }
        System.out.println(res);
    }
}
